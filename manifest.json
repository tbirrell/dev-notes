{
  "manifest_version": 2,
  "name": "DevNotes",
  "version": "1.0",
  "author": "tbirrell",
  "background" : {
    "scripts": ["jquery-3.0.0.min.js", "background.js"]
  },
  "permissions": [
    "contextMenus",
    "cookies",
    "webNavigation",
    "*://*/*"
  ],
  "browser_action": {
    "default_title": "Click to enable Devnotes for this domain"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.0.0.min.js", "jquery-ui.min.js", "main.js"],
      "css":["main.css"]
    }
  ]
}